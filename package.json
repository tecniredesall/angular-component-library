{
  "name": "gc-component-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky install",
    "grid": "sass ./src/assets/bootstrap-custom.scss  ./projects/gc-crm/gc-component-lib/gc-cl-assets/styles/gc-cl-bootstrap-custom.scss --style compressed",
    "grid:dev": "sass ./src/assets/bootstrap-custom.scss  ./projects/gc-crm/gc-component-lib/gc-cl-assets/styles/gc-cl-bootstrap-custom.scss",
    "lib": "npm run grid && ng build @gc-crm/gc-component-lib",
    "lib:dev": "npm run grid:dev && ng build @gc-crm/gc-component-lib --watch",
    "publish-lib": "npm run lib && npm publish dist/gc-crm/gc-component-lib",
    "install-lib:local": "npm run lib && npm un dist/gc-crm/gc-component-lib && npm i dist/gc-crm/gc-component-lib && npm link dist/gc-crm/gc-component-lib"
  },
  "lint-staged": {
    "*.{js,ts,tsx,html}": [
      "prettier --write --ignore-unknown",
      "ng-lint-staged lint --fix --",
      "ng-lint-staged lint --"
    ],
    "*.json": [
      "prettier --write --ignore-unknown"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@gc-crm/gc-component-lib": "^12.10.7",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@sentry/angular": "^7.15.0",
    "@sentry/tracing": "^7.15.0",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "5.1.3",
    "lodash": "^4.17.21",
    "ngx-translate-multi-http-loader": "^7.0.1",
    "rxjs": "~6.6.0",
    "text-mask-addons": "^3.8.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-lint-staged": "^12.0.4",
    "ng-packagr": "^14.2.2",
    "prettier": "^2.7.1",
    "typescript": "~4.6.4"
  }
}
